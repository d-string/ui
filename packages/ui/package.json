{
  "name": "@acme/ui",
  "version": "0.0.0",
  "exports": {
    "./styles.css": "./dist/styles.css",
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o dist/styles.css && tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "concurrently \"tailwindcss -i ./src/styles.css -o dist/styles.css --watch\" \"tsup src/index.ts --format esm,cjs --watch --dts --external react\"",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/tailwindcss-config": "*",
    "@repo/typescript-config": "*",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "concurrently": "^9.0.1",
    "eslint": "^9.12.0",
    "postcss": "^8.4.47",
    "tailwind-merge": "^2.5.3",
    "tailwindcss": "^3.4.13",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "@tabler/icons-react": "^3.19.0"
  }
}
